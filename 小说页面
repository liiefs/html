    function winSetHP() {
        var name = document.title;
        if (document.all) {
            document.body.style.behavior = 'url(#default#homepage)';
            document.body.setHomePage(name);
        } else {
            swal({
                title: "提示",
                text: '浏览器不支持此操作, 请手动设为首页'
            });
        }
    }
    function winAddFav() {
        var domain = window.location.href;
        var name = document.title;
        try {
            window.external.addFavorite(domain, name);
        } catch (e) {
